public class PingPong {
	public static void main(String args[]){
	
	for (int x = 0; x < 100; x++){
		if ((x % 3 == 0)&&(x % 5 == 0))
			System.out.println("ping pong");
		else if (x % 3 == 0)
			System.out.println("ping");
		else if (x % 5 == 0)
			System.out.println("pong");
		else
			System.out.println(x);
	}	
	}
}
