public class PrimeNumber {
	public static void main(String[] args){
		System.out.print(isPrime(29));
		
		System.out.print(isPrime(34));
	}
	    public static boolean isPrime(int number) {
	        
	    for (int i = 2; i <= number - 1; i++) {
	        if (number % i == 0)
	            return false;
	    }
	return true;
	}
}
